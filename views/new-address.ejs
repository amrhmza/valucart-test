    <div class="container">
        <h4>Add new address</h4>
        <div class="panel1 panel-default">
            <div class="panel-body">
                <form style="margin-top: 20px">
                    <div class="form-group marg-btm1">
                        <input type="text" id="name" class="form-control form-control1">
                        <label class="form-control-placeholder" for="name">FULL NAME</label>
                    </div>
                    <div class="row form-align  marg-btm1">
                        <div class="col-sm-1 col-xs-2">
                            <div class="form-group">
                                <label class="phone-align" for="phone">+971</label>
                            </div>
                        </div>
                        <div class="col-sm-11 col-xs-10">
                            <div class="form-group" style="margin-top: 35px;">
                                <input type="text" id="phno" class="form-control form-control1">
                                <label class="form-control-placeholder" for="phno">PHONE NUMBER</label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group marg-btm1" style="margin-top: 35px;">
                        <input type="text" id="flat" class="form-control form-control1">
                        <label class="form-control-placeholder" for="flat">FLAT/HOUSE/FLOOR/BUILDING</label>
                    </div>
                    <div class="form-group  marg-btm1" style="margin-top: 35px;">
                        <input type="text" id="street" class="form-control form-control1">
                        <label class="form-control-placeholder" for="street">STREET/LOCALITY</label>
                    </div>
                    <div class="form-group marg-btm1" style="margin-top: 35px;">
                        <input type="text" id="city" class="form-control form-control1">
                        <label class="form-control-placeholder" for="city">CITY</label>
                    </div>
                    <div class="form-group btn-bottom text-right visible-lg visible-md hidden-sm hidden-xs">
                        <button class="btn-pwd" style="width:200px;margin-right: 10px;">Save</button>
                        <button class="btn-cnl" style="width:200px">Cancel</button>
                    </div>
                    <div class="form-group btn-bottom visible-xs visible-sm hidden-lg hidden-md">
                        <button class="btn-pwd">Save</button>
                        <button class="btn-cnl">Cancel</button>
                    </div>
