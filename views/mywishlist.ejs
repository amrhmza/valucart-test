<% include template/header %>
    <!--    Breadcrumb-->
    <div class="container">
        <ul class="breadcrumb">
            <li>
                <a href="home.html">Home</a>
            </li>
            <li>
                <a href="#">My Wishlist</a>
            </li>
        </ul>
        <h3>My Wishlist</h3>
    </div>
    <!--    Breadcrumb-->
    <!--    Product list starts here-->
    <div class="container-fluid mycart">
        <div class="container productlistpagec">
            <div class="row">
                <div class="col-md-12 no-pad">
                    <% 
                    let wishList= data.results; 
                    if(wishList!=""){
                        wishList.forEach(function(pro){
                    %>
                    <div class="panel panel-default productpanel">
                        <div class="panel-body">
                            <div class="row" style="margin-bottom: 40px">
                                <div class="col-xs-4">
                                    <p class="discountprice">-10%</p>
                                    <img src="<%= pro.p_image %>" class="img-responsive productimg">
                                    <div class="col-xs-6 favourite">
                                        <i class="fa fa-trash-o trash" aria-hidden="true"></i>
                                        <!-- <i class="fa fa-heart wishheart" data-type="add" ng-click="addwish(<%=pro.pd_name%>, false, $event.target)"></i> -->
                                    </div>
                                </div>
                                <div class="col-xs-8">
                                    <h4><a href=""><%= pro.pd_name%></a></h4>
                                    <h6 class="detail-text">- Nutella hazelnut chocolate spread
                                        <strong>X 1</strong>
                                    </h6>
                                    <h6 class="detail-text">- Kellogg’s frosties
                                        <strong>X 1</strong>
                                    </h6>
                                    <h6 class="detail-text">- Hershey’s strawberry syrup
                                        <strong>X 1</strong>
                                    </h6>
                                    <div class="col-xs-12">
                                        <h6 class="rate">
                                            <span><%= pro.p_price%> AED</span> <%= pro.p_price%> AED</h6>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <%
                        });
                    }
                    %>
                    <div id="shop-desktop">
                        <a href="/">
                            <h5 class="continue-text">
                                <i class="fa fa-chevron-left" aria-hidden="true" style="margin-right: 5px"></i> continue Shopping</h5>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--     Product list ends here-->
    <% include template/footer %>