</head>

<body>
    <!--Header Desktop / this will hide on mobile -->
    <div class="container-fluid headertop">
        <div class="container">
            <ul class="list-inline headerlist">
                <li style="border-right: 1px solid #9b9b9b;">
                    <a href="#">
                        <p style="margin: 0px;padding-right: 5px;">
                            <img class="img-responsive topnavlocationimg" src="img/phone.png">+91-9876543210</p>
                    </a>
                </li>
                <li style="border-right: 1px solid #9b9b9b;">
                    <a href="/city">
                        <p style="margin: 0px;padding-right: 5px;">
                            <img class="img-responsive topnavlocationimg" src="img/locationtop.png"> <span class="current_city">Al Karama, Dubai</span>
                            <i class="fa fa-chevron-down"></i>
                        </p>
                    </a>
                </li>
                <% 
                if(cookies){
                var userData=JSON.parse(cookies);
                if(userData.status=="success"){
                %>
                <li style="margin-bottom: 0px;">
                    <div class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img class="img-responsive topnavlocationimg" src="img/user.png"><%=(userData.username).toUpperCase();%>
                            <i class="fa fa-chevron-down"></i>
                        </a>
                        <ul class="dropdown-menu">
                            <li>
                                <a href="myorders">My orders</a>
                            </li>
                            <li>
                                <a href="profile">Profile</a>
                            </li>
                            <li>
                                <a href="addressbook">Address Book</a>
                            </li>
                            <li>
                                <a href="change-password">Change Password</a>
                            </li>
                            <li>
                                <a href="#">Setting</a>
                            </li>
                            <li>
                                <a href="myshopping-dashboard">Shopping list</a>
                            </li>
                            <li>
                                <a href="mybundles">My bundles</a>
                            </li>
                            <li>
                                <a href="#" class="vcartLogout">Logout</a>
                            </li>
                        </ul>
                    </div>

                </li>
                <%
                    }
                }
                else{
                %>
                <li>
                    <a href="#" data-toggle="modal" data-target="#myModal">
                        <img class="img-responsive topnavlocationimg" src="img/user.png">Login/Signup</a>
                </li>   
                <%    
                }
                %>
            </ul>
        </div>
    </div>
    <!--    Header-->
    <!--    Navigation starts here-->
    <div class="container-fluid navtop">
        <div class="container">
            <div class="row">
                <div class="hidden-md hidden-sm hidden-lg vivible-xs col-xs-3">
                    <span style="font-size:30px;cursor:pointer;" onclick="openNav()">
                        <i class="fa fa-ellipsis-v"></i>
                    </span>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-6">
                   <a href=""><img src="img/logo.png" class="img-responsive navbar-brand" alt="valucart-logo"></a></div>
                   <% 
                   if(cookies){
                    var userData=JSON.parse(cookies);
                   if(userData.status=="success"){
                   %>
                    <div class="col-md-6 col-sm-6 col-xs-3">
                    <ul class="list-inline navlist">
                        <li class="hidden-xs">
                            <a href="mywishlist">
                                <img src="img/wishlist.png" class="img-responsive navbar-brand" alt="valucart-logo" style="margin-right: 10px;">
                            </a>
                        </li>
                        <li>
                            <a href="/cart">
                                <img src="img/cart.png" class="img-responsive navbar-brand" alt="valucart-logo">
                            </a>
                        </li>
                    </ul>
                </div>
                <% }
            } %>
            </div>
        </div>
    </div>
    <% if(typeof search=="undefined"){%>
    <div class="container-fluid navtop2">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-4 hidden-xs">
                    <nav class="navbar navbar-static-top line-navbar-two">
                        <div class="container" style="padding-left: 0px;">
                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse" id="line-navbar-collapse-2">
                                <ul class="nav navbar-nav lnt-nav-mega">
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
                                            Categories
                                        </a>
                                        <div class="dropdown-menu" role="menu">
                                            <div class="lnt-dropdown-mega-menu">
                                                <!-- List of categories -->
                                                <ul class="lnt-category list-unstyled">
                                                    <% 
                                                    let menuList= (typeof menudata != 'undefined')?menudata.response:[];
                                                    var no=0;
                                                    if (menuList) {
                                                        menuList.forEach(function(cat){
                                                            no++;
                                                            var cname= cat.c_name;
                                                            var cat_url_name = cname.trim().replace(/\s+/g, '-').toUpperCase();
                                                            var catUrl= process.env.BASEURL+'product-listing/'+cat.c_id+'/'+cat_url_name;
                                                               
                                                    %>
                                                    <li class="<%= (no==1)?'active':''; %>">
                                                        <a href="<%= catUrl;%>" data-root="#subcategory-<%= cat.c_id;%>"><%= cat.c_name;%></a>
                                                    </li>
                                                    <%
                                                        });
                                                    }
                                                    %>                                                    
                                                </ul>
                                                <!-- Subcategory and carousel wrap -->
                                                <div class="lnt-subcategroy-carousel-wrap container-fluid">
                                                    <% 
                                                    var no=0;
                                                    if (menuList) {
                                                        menuList.forEach(function(cat){
                                                            no++;
                                                    %>
                                                    <div id="subcategory-<%= cat.c_id;%>" class="<%= (no==1)?'active':''; %>">
                                                        <!-- Sub categories list-->
                                                        <div class="lnt-subcategory col-sm-6 col-md-6">
                                                            <!--<h3 class="lnt-category-name">Home, garden and tools</h3>-->
                                                            <%
                                                            let subcatList = cat.sub_cat; 
                                                            if (subcatList) {
                                                            %>
                                                            <ul class="list-unstyled col-sm-12">
                                                                <%
                                                                subcatList.forEach(function(scat){
                                                                    var scname= scat.c_name;
                                                                    var scat_url_name = scname.trim().replace(/\s+/g, '-').toUpperCase();
                                                                    var scatUrl= process.env.BASEURL+'product-listing/'+scat.c_parent_id+'/'+scat_url_name+'#!?sub_cat='+scat.c_id;
                                                                %>
                                                                <li>
                                                                    <a href="<%= scatUrl%>"><%=scat.c_name%></a>
                                                                </li>
                                                                <%
                                                                });
                                                                %>
                                                                <!-- <li>
                                                                    <a style="color: #14bdcd; text-decoration: underline; font-weight: 500;" href="#">See all New & trending</a>
                                                                </li> -->
                                                            </ul>
                                                            <%
                                                            }
                                                            %>
                                                        </div>
                                                    </div>
                                                    <%
                                                        });
                                                    }
                                                    %>
                                                    <!-- /subcategory ends -->
                                                </div>
                                                <!-- /.lnt-subcategroy-carousel-wrap -->
                                            </div>
                                            <!-- /.lnt-dropdown-mega-menu -->
                                        </div>
                                        <!-- /.dropdown-menu -->
                                    </li>
                                    <!-- /.dropdown -->
                                </ul>
                                <!-- /.lnt-nav-mega -->
                                <!-- /.lnt-search-form -->
                                <!-- /.lnt-shopping-cart -->
                            </div>
                            <!-- /.navbar-collapse -->
                        </div>
                        <!-- /.container -->
                    </nav>
                    <!--nav navbar-nav-->
                </div>
                <div class="col-md-5 col-sm-4 hidden-xs text-left" style="padding-top: 8px; z-index: 1000;">
                    <a href="#">
                        <img src="img/exclusive.png" class="img-responsive exclusiveimg" alt="Valucart Exclusives"> Valucart Exclusives</a>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-6" style="z-index: 1000;">
                    <div id="sb-search" class="sb-search column hidden-md hidden-sm hidden-lg">
                        <form class="mobsearchform">
                            <input class="sb-search-input" placeholder="Enter your search term..." type="text" value="" name="search" id="search">
                            <input class="sb-search-submit" type="submit" value="">
                            <span class="sb-icon-search"></span>
                        </form>
                    </div>

                    <div class="input-group hidden-xs">
                        <input type="text" class="form-control desktopsearch" placeholder="Search for...">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button">
                                <img src="img/searchicon.png" class="img-responsive searchicon" /> </button>
                        </span>
                    </div>
                </div>
                <div class="hidden-md hidden-sm hidden-lg vivible-xs col-xs-6">
                    <a href="/city">
                        <p class="navlocation">
                            <img class="img-responsive navlocationimg" src="img/location.png"> Al Karama, Dubai</p>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <!-- mobile menu -->
    <div id="mobileview" class="mobileview1" style="display: none">
        <div id="mySidenav" class="sidenav" style="overflow-y: scroll;">
            <div class="sideheader">
                <img src="img/blacklogo.png" class="img-responsive navbar-brand-mob" alt="valucart-logo">
                <a href="javascript:void(0)" style="display: inline-block;" class="closebtn" onclick="closeNav()">&times;</a>
            </div>
            <div class="tab">
                <button class="tablinks button1" onclick="openCity(event, 'account')" id="defaultOpen">
                    <img class="svg" src="img/user.svg" alt="" />
                    <br>My Account</button>
                <button class="tablinks button1" onclick="openCity(event, 'categories')">
                    <img class="svg" src="img/shop.svg" alt="" />
                    <br>Categories</button>
                <button class="tablinks button1" onclick="openCity(event, 'offers')">
                    <img class="svg" src="img/valucart.svg" alt="" />
                    <br>ValuCart Exclusives</button>
            </div>
            <div id="account" class="tabcontent">
                <a href="#">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <a href="#" data-toggle="modal" data-target="#myModal">
                                <h4 class="panel-title panels">Login
                                    <i class="fa fa-chevron-right" aria-hidden="true"></i>
                                </h4>
                            </a>
                        </div>
                    </div>
                </a>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Sign up
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> My cart
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Wishlist
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
            </div>
            <div id="categories" class="tabcontent">
                <% 
                    var no=0;
                    if (menuList) {
                        menuList.forEach(function(cat){
                            no++;
                            var cname= cat.c_name;
                            var cat_url_name = cname.trim().replace(/\s+/g, '-').toUpperCase();
                            var catUrl= process.env.BASEURL+'product-listing/'+cat.c_id+'/'+cat_url_name;
                               
                %>     
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels" data-href="<%= catUrl;%>"> <%= cat.c_name;%>
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <%
                        });
                    }
                 %> 
            </div>
            <div id="offers" class="tabcontent">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> News & Trending
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels">Holiday shop
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Chilled
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Food & Cupboard
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Drinks
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> health & Beauty
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Baby & toddler
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> household
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> pet
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title panels"> Home & Outdoor
                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
                        </h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% } %>
